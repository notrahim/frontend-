{"ast":null,"code":"import dateMixin from '../../../mixins/dateMixin';\nexport default {\n  props: [\"id\"],\n  data: function data() {\n    return {\n      date: null,\n      on: false,\n      ShowGerant: false,\n      menu: false,\n      SwitchPouvoir: false,\n      activator: null,\n      AutreNom: null,\n      AutrePrenom: null,\n      AutreNat: null,\n      AutreNeDpt: null,\n      AutreNeVille: null,\n      AutreNePays: null,\n      AutreRue: null,\n      AutreZip: null,\n      AutreVille: null\n    };\n  },\n  computed: {\n    postData: function postData() {\n      var data = {\n        AutrePouvoirLimite: this.SwitchPouvoir,\n        AutreBirth_nom: this.AutreNom,\n        AutreBirth_prenom: this.AutrePrenom,\n        AutreBirth_nationalite: this.AutreNat,\n        AutreBirth_date: this.date,\n        AutreBirth_departement: this.AutreNeDpt,\n        AutreBirth_commune: this.AutreNeVille,\n        AutreBirth_pays: this.AutreNePays,\n        AutreAdress_rue: this.AutreRue,\n        AutreAdress_codepostal: this.AutreZip,\n        AutreAdress_commune: this.AutreVille\n      };\n      return data;\n    }\n  },\n  mixins: [dateMixin]\n};","map":{"version":3,"mappings":"AA6IA;AAEA;EACAA,aADA;EAGAC,IAHA,kBAGA;IACA;MACAC,UADA;MAEAC,SAFA;MAGAC,iBAHA;MAIAC,WAJA;MAKAC,oBALA;MAMAC,eANA;MAOAC,cAPA;MAQAC,iBARA;MASAC,cATA;MAUAC,gBAVA;MAWAC,kBAXA;MAYAC,iBAZA;MAaAC,cAbA;MAcAC,cAdA;MAeAC;IAfA;EAiBA,CArBA;EAsBAC;IACAC,QADA,sBACA;MACA;QACAC,sCADA;QAEAC,6BAFA;QAGAC,mCAHA;QAIAC,qCAJA;QAKAC,0BALA;QAMAC,uCANA;QAOAC,qCAPA;QAQAC,iCARA;QASAC,8BATA;QAUAC,qCAVA;QAWAC;MAXA;MAaA;IACA;EAhBA,CAtBA;EAwCAC;AAxCA","names":["props","data","date","on","ShowGerant","menu","SwitchPouvoir","activator","AutreNom","AutrePrenom","AutreNat","AutreNeDpt","AutreNeVille","AutreNePays","AutreRue","AutreZip","AutreVille","computed","postData","AutrePouvoirLimite","AutreBirth_nom","AutreBirth_prenom","AutreBirth_nationalite","AutreBirth_date","AutreBirth_departement","AutreBirth_commune","AutreBirth_pays","AutreAdress_rue","AutreAdress_codepostal","AutreAdress_commune","mixins"],"sourceRoot":"src/components/popup/popup","sources":["AutreGerant.vue"],"sourcesContent":["<template>\n    <v-card>\n      <v-row class=\"pa-2 pt-0\">\n        <v-card-title>Une autre personne que le gérant peut engager la société</v-card-title>\n        <v-checkbox color=\"accent\" v-model=\"ShowGerant\"></v-checkbox>\n      </v-row>\n      <v-container v-if=\"ShowGerant\">\n      <h4>Infos personnelles :</h4>\n      <v-row>\n        <v-col cols=\"4\">\n          <v-text-field\n            label=\"Nom\"\n            v-model=\"AutreNom\"\n            dense\n            outlined\n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"4\">\n          <v-text-field\n            label=\"Prenom\"\n            v-model=\"AutrePrenom\"\n            dense\n            outlined\n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"4\">\n          <v-text-field\n            label=\"Nationalite\"\n            v-model=\"AutreNat\"\n            dense\n            outlined\n          ></v-text-field>\n        </v-col>\n      </v-row>\n      <v-row>\n        <v-col cols=\"4\">\n          <div>\n          <v-menu\n            ref=\"menu\"\n            v-model=\"menu\"\n            :close-on-content-click=\"false\"\n            transition=\"scale-transition\"\n            color=\"accent\"\n            offset-y\n            min-width=\"auto\"\n          >\n            <template v-slot:activator=\"{ on, attrs }\">\n              <v-text-field\n                v-model=\"date\"\n                label=\"Date de naissance\"\n                prepend-inner-icon=\"mdi-calendar\"\n                readonly\n                color=\"accent\"\n                dense\n                outlined\n                v-bind=\"attrs\"\n                v-on=\"on\"\n              ></v-text-field>\n            </template>\n            <v-date-picker\n              v-model=\"date\"\n              color=\"accent\"\n              :active-picker.sync=\"activePicker\"\n              :max=\"(new Date(Date.now() - (new Date()).getTimezoneOffset() * 60000)).toISOString().substr(0, 10)\"\n              min=\"1940-01-01\"\n              @change=\"save\"\n            ></v-date-picker>\n          </v-menu>\n        </div>\n        </v-col>\n        <v-col cols=\"2\">\n          <v-text-field\n            label=\"Departement\"\n            v-model=\"AutreNeDpt\"\n            dense\n            outlined\n            \n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"3\">\n          <v-text-field\n            label=\"Commune\"\n            dense\n            v-model=\"AutreNeVille\"\n            outlined\n            \n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"3\">\n          <v-text-field\n            label=\"Pays\"\n            v-model=\"AutreNePays\"\n            dense\n            outlined\n            \n          ></v-text-field>\n        </v-col>\n      </v-row>\n      <h4>Adresse :</h4>\n      <v-row>\n        <v-col cols=\"4\">\n          <v-text-field\n            label=\"Rue\"\n            v-model=\"AutreRue\"\n            dense\n            outlined\n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"4\">\n          <v-text-field\n            label=\"Code Postal\"\n            v-model=\"AutreZip\"\n            dense\n            outlined\n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"4\">\n          <v-text-field\n            label=\"Commune\"\n            dense\n            v-model=\"AutreVille\"\n            outlined\n          ></v-text-field>\n        </v-col>\n      </v-row>\n      <v-container>\n      <v-row align=\"center\">\n          <h4>Pouvoir limité à l'établissement</h4>\n          <v-checkbox\n            v-model=\"SwitchPouvoir\"\n            color=\"accent\"\n          >\n          </v-checkbox>\n      </v-row>\n      </v-container>\n    </v-container>\n    </v-card>\n</template>\n\n<script>\n\nimport dateMixin from '../../../mixins/dateMixin'\n\nexport default {\nprops: [\"id\"],\n\ndata() {\n  return {      \n    date: null,\n    on: false,\n    ShowGerant: false,\n    menu: false,\n    SwitchPouvoir: false,\n    activator: null,\n    AutreNom: null,\n    AutrePrenom: null,\n    AutreNat: null,\n    AutreNeDpt: null,\n    AutreNeVille: null,\n    AutreNePays: null,\n    AutreRue: null,\n    AutreZip: null,\n    AutreVille: null,\n  };\n},\ncomputed: {\n  postData() {\n    let data = {\n      AutrePouvoirLimite: this.SwitchPouvoir,\n      AutreBirth_nom: this.AutreNom,\n      AutreBirth_prenom: this.AutrePrenom,\n      AutreBirth_nationalite: this.AutreNat,\n      AutreBirth_date: this.date,\n      AutreBirth_departement: this.AutreNeDpt,\n      AutreBirth_commune: this.AutreNeVille,\n      AutreBirth_pays: this.AutreNePays,\n      AutreAdress_rue: this.AutreRue,\n      AutreAdress_codepostal: this.AutreZip,\n      AutreAdress_commune: this.AutreVille,\n    };\n    return data;\n  },\n},\nmixins: [dateMixin]\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}